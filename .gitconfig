[alias]
  a = add
  ap = add --patch
  b = branch
  c = commit
  ca = commit --amend
  ch = checkout
  reb = rebase
  rebc = rebase --continue
  reba = rebase --abort
  s = status -u
  scrap = checkout --
  staged = diff --cached
  unstage = reset HEAD .
  undo = reset --soft HEAD~
  tags = tag --sort=taggerdate
  tree = log --pretty=format:'%C(yellow)commit %H%Creset%C(green)%d%Creset%nCommitter: %cn <%ce>%n%n%s%n%cr%n%n%b' --graph
  minitree = log --oneline --graph
  last = log -1
  size = count-objects -vH
  where = log --oneline --branches
  refresh = remote update --prune
[core]
  editor = /usr/bin/vim
[branch]
  autosetuprebase = always
